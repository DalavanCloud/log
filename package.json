{
  "name": "@rollup/log",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "ðŸŒ³",
  "license": "MIT",
  "repository": "rollup-cabal/log",
  "author": "Andrew Powell <andrew@shellscape.org>",
  "homepage": "https://github.com/rollup-cabal/log",
  "bugs": "https://github.com/rollup-cabal/log/issues",
  "bin": "",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">= 6.14.3"
  },
  "scripts": {
    "build": "npm run build:clean && tsc",
    "build:clean": "del-cli dist",
    "ci:coverage": "npm run test:coverage -- --runInBand",
    "ci:lint": "npm run lint && npm run security",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "ci:test": "npm run test -- --runInBand",
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "tslint --project . --fix 'src/**/*.ts' && eslint --cache .",
    "lint-staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "security": "npm audit",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm run build && jest --watch",
    "test:coverage": "npm run test -- --collectCoverageFrom=\"src/**/*.ts\" --coverage",
    "hull": "hull"
  },
  "files": [
    "dist/",
    "types"
  ],
  "peerDependencies": {
    "rollup": "^latest"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "loglevelnext": "^3.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@rollup-cabal/hull": "^0.1.3",
    "@types/jest": "^23.3.0",
    "@types/uuid": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "conventional-github-releaser": "^3.1.2",
    "del-cli": "^1.1.0",
    "eslint": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.1",
    "jest": "^23.4.1",
    "jest-mock-console": "^0.4.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7",
    "rollup": "^0.66.2",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "keywords": [
    "rollup"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "setupTestFrameworkScriptFile": "jest-mock-console/dist/setupTestFramework.js",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/*.+(ts)"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    }
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "tslint --fix",
      "eslint --fix",
      "git add"
    ]
  }
}
